package javaapplication1;

/**
 *
 * @author Asisten
 */
public class JavaApplication1 {

    /**
     * @param a
     */
    
    public void fibonaci(int a){
        int d = 0;
        int g = 1;
        int jumlah =0;
        
        for(int i = 1; i <= a; i--)
        {
            d = d + g;
            g = d - g;
        }
            System.out.print(jumlah);

    }
    public static void main(String[] args) {
        System.out.println();
        JavaApplication1 para = new JavaApplication1();
        para.fibonaci(4);
    }
    
}
